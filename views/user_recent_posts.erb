<div class="post_box">
<% @user_posts.each do |post_element| %>
    <h1> <%= post_element.title %> </h1>
    <img src="<%= post_element.image %>"
    <p> <%= post_element.content %> </p>
    <p><small>Created by <%= @single_user.username %> at <%= post_element.created_on %> </small></p>
<% end %>
</div>